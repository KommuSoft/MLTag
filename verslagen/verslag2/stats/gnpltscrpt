set term pdf size 7.5,4.5
set output "vectorClassifouter.pdf"
plot "./outerstatsVectorClassif.dat" using 1:6 title "precision", "./outerstatsVectorClassif.dat" using 1:7 title "recall", "./outerstatsVectorClassif.dat" using 1:8 title "accuracy", "./outerstatsVectorClassif.dat" using 1:9 title "F-measure", "./outerstatsVectorClassif.dat" using 1:10 title "hamming loss"

set output "vectorClassifinner.pdf"
plot "./innerstatsVectorClassif.dat" using 1:6 title "precision", "./innerstatsVectorClassif.dat" using 1:7 title "recall", "./innerstatsVectorClassif.dat" using 1:8 title "accuracy", "./innerstatsVectorClassif.dat" using 1:9 title "F-measure", "./innerstatsVectorClassif.dat" using 1:10 title "hamming loss"



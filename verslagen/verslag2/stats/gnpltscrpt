set output "j48outer.pdf"
plot "./outerstatsJ48.dat" using 1:6 title "precision", "./outerstatsJ48.dat" using 1:7 title "recall", "./outerstatsJ48.dat" using 1:8 title "accuracy", "./outerstatsJ48.dat" using 1:9 title "F-measure", "./outerstatsJ48.dat" using 1:10 title "hamming loss"

